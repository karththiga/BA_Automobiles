df <- read.csv("Prestige_New.csv")
# Minimum income
min_income <- min(df$income, na.rm = TRUE)
# Maximum income
max_income <- max(df$income, na.rm = TRUE)
# Mean income
mean_income <- mean(df$income, na.rm = TRUE)
# Median income
median_income <- median(df$income, na.rm = TRUE)
# Mode function
get_mode <- function(x) {
ux <- unique(x)
ux[which.max(tabulate(match(x, ux)))]
}
# Mode income
mode_income <- get_mode(df$income)
# Print all results
min_income
max_income
mean_income
median_income
mode_income

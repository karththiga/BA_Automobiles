#Load required Package
library(ggplot2)
# Set seed for reproducibility
set.seed(123)
#Create two skewed datasets
data<-data.frame(
value=c(rexp(300,rate = 0.5),   #right-skewed
-rexp(300,rate = 0.5)+10),# left skewed
type=rep(c("Right skewed","Left skewed"),each=300)
)
#------------------------------------
#histogram to show skewness
#------------------------------------
ggplot(data,aes(x=value,fill = type)) +
geom_histogram(color="black",bins = 30,alpha=0.7) +
facet_wrap(~type,scales="free") +
labs(title = "Right vs Left skewness",
x="Value",y="Frequency") +
theme_minimal() +
scale_fill_manual(values = c("Right skewed"="skyblue","Left skewed"="tomato"))
#------------------------------------
#Boxplot to visualize skewness
#------------------------------------
ggplot(data,aes(x=type,y=value,fill = type)) +
geom_boxplot(alpha=0.7) +
labs(title = "Box plots showing laeft and right skeness",
x="",y="value") +
theme_minimal() +
scale_fill_manual(values = c("Right skewed"="skyblue","left skewed"="red"))
#outliers/extreme points that are far exceeded to boxplot -they are not used in analysis(which will be greater than max and lessthan min)
